{
  "version": "",
  "description": "A copycat implementation of Arc'teryx product pages, using React & css to mimic the look and feel of a professional website.",
  "author": "Amanda Post [Front-End] | Kristina Taing [Back-End]",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "start": "nodemon server/index.js",
    "build": "webpack -d --watch",
    "seed-mongodb": "mongoimport --db Product --collection products --file ./database-mongodb/seed/data.json",
    "seed-postgresql": "node database-postgresql/seed.js",
    "generate-seed:mongodb": "node --max-old-space-size=8192 database-mongodb/seed/seeder.js",
    "generate-seed:postgresql": "node --max-old-space-size=8192 database-postgresql/seed/seeder.js",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "file-loader": "^4.0.0",
    "mongoose": "^5.6.7",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.10",
    "pg": "^7.12.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-icons": "^3.7.0",
    "sequelize": "^5.10.3",
    "url-loader": "^2.0.1",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
    "jest": "^24.8.0"
  }
}
